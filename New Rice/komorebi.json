{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.json",
  "window_hiding_behaviour": "Cloak",
  "mouse_follows_focus": false,
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 10,
  "default_container_padding":2,
  "border": false,
  "border_width": 4,
  "border_offset": -1,
  "transparency": true,
  "transparency_alpha": 225,
  "animation": {
    "enabled":true,
    "fps": 165,
    "duration": 250,
    "style":"EaseOutExpo"
  },
  "border_colours": {
    "single": "#929292ff",
    "unfocused": "#363636",
    "monocle": "#929292ff",
    "floating": "#929292ff"
  },
  "monitors": [
    {
      "workspaces": [
        {
          "name": "一",
          "layout": "BSP"
        },
        {
          "name": "二",
          "layout": "BSP"
        },
        {
          "name": "三",
          "layout": "BSP"
        },
        {
          "name": "四",
          "layout": "BSP"
        },
        {
          "name": "五",
          "layout": "BSP"
        },
        {
          "name": "六",
          "layout": "BSP"
        },
        {
          "name": "七",
          "layout": "BSP"
        },
        {
	  "name": "八",
	  "layout": "BSP"
	},
	{
          "name": "九",
	  "layout": "BSP"
	}
      ]
    }
  ]
}
